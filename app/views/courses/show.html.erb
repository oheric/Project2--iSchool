<h1>Roster</h1>
<table>
<thead>
  <tr>
   <th>Last Name</th>
   <th>First Name</th>
   <th>Notes  </th>
  </tr>
</thead>
<%@students.each do |s| %>
   <tr>
   <td><%= s.lastname %></td>
   <td><%= s.firstname %></td>
   <td><%= s.notes %></td>




  


 <td><%= link_to 'More', show_path(:student => s.id) %></td>
 <td><%= link_to 'Edit', edit_path(:student => s.id) %></td>
 <td><%= link_to 'Delete Student', delete_path(:student => s.id), method: :delete, data: {confirm: 'Are you sure?'} %></td>

   </tr>
<% end %>
</table>




<%= link_to 'Add a Student', new_student_course_path %><br>
<%= link_to 'Back to Courses', courses_path %>


